{
  "hosting": [
    {
      "public": "build",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "target": "github-issues-section",
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "public": "storybook-static",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "target": "github-issues-section-storybook"
    }
  ]
}
